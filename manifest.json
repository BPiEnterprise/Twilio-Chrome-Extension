{
    "name": "Twilio API Tool",
    "version": "0.2",
    "description": "This tool makes it easy to try the Twilio API from Chrome.",
    "browser_action": {
        "default_icon": "img/logomash.png",
		"popup": "popup.html"
    },
	"options_page": "options.html",
    "permissions": [
		"https://api.twilio.com/*"
    ],
    "icons": {
        "128": "img/logomash128.png",
        "48": "img/logomash48.png"
    }
}
